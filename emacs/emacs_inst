libharfbuzz-dev:
apt-mark unhold libicu72
apt-mark unhold libglib2.0-0
apt-mark unhold libglib2.0-bin
apt install libharfbuzz-dev

libxml2-dev:
apt install libxml2
apt install libxml2-dev


libgnutls28-dev:
may be you need just unhold libgnutls28(without dev)
libgnutls-dane0
libgnutls-openssl27
libgnutls30
libgnutlsxx30
